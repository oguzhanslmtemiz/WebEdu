<!-- header section strats -->
<header class="header_section">
    <div class="container-fluid">
        <nav class="navbar navbar-expand-lg custom_nav-container ">
            <a class="navbar-brand" href="/">
                <h3>
                    Joson
                </h3>
                <span> college</span>
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse ml-auto" id="navbarSupportedContent">
                <ul class="navbar-nav  ml-auto">
                    <li class="nav-item <%= page_name === 'index' && 'active' %>">
                        <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item <%= page_name === 'about' && 'active' %>">
                        <a class="nav-link" href="/about"> About </a>
                    </li>
                    <li class="nav-item <%= page_name === 'courses' && 'active' %>">
                        <a class="nav-link" href="/courses"> Courses </a>
                    </li>
                    <li class="nav-item <%= page_name === 'contact' && 'active' %>">
                        <a class="nav-link" href="/contact">Contact us</a>
                    </li>
                    <% if(userIN){ %>
                    <li class="nav-item <%= page_name === 'events' && 'active' %>">
                        <a class="nav-link" href="/events"> Events </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link logout" href="/users/logout">
                            <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor"
                                class="bi bi-box-arrow-right" viewBox="0 0 16 16">
                                <path fill-rule="evenodd"
                                    d="M10 12.5a.5.5 0 0 1-.5.5h-8a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h8a.5.5 0 0 1 .5.5v2a.5.5 0 0 0 1 0v-2A1.5 1.5 0 0 0 9.5 2h-8A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h8a1.5 1.5 0 0 0 1.5-1.5v-2a.5.5 0 0 0-1 0v2z">
                                </path>
                                <path fill-rule="evenodd"
                                    d="M15.854 8.354a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L14.293 7.5H5.5a.5.5 0 0 0 0 1h8.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3z">
                                </path>
                            </svg>
                        </a>
                    </li>
                    <% } %>
                    <% if(!userIN){ %>
                    <li class="nav-item <%= ((page_name === 'login') || (page_name === 'register')) && 'active' %>">
                        <a class="nav-link" href="/login">Login</a>
                    </li>
                    <% } %>
                </ul>
            </div>
        </nav>
    </div>
</header>
<!-- end header section -->